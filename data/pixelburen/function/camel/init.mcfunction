# Camel breeding initialization

# Add scoreboard object to store calculation values
scoreboard objectives add pixelburen.camel dummy

# Fake players used as registers for default values
scoreboard players set #PIXELBUREN_CAMEL_MIN_SPEED pixelburen.camel 800
scoreboard players set #PIXELBUREN_CAMEL_MAX_SPEED pixelburen.camel 2380
scoreboard players set #PIXELBUREN_CAMEL_TWO pixelburen.camel 2
scoreboard players set #PIXELBUREN_CAMEL_THREE pixelburen.camel 3
scoreboard players set #PIXELBUREN_CAMEL_TEN pixelburen.camel 10
scoreboard players set #PIXELBUREN_CAMEL_FIVE_HUNDRED pixelburen.camel 500
scoreboard players set #PIXELBUREN_CAMEL_THOUSAND pixelburen.camel 1000
scoreboard players set #PIXELBUREN_CAMEL_NEGATIVE pixelburen.camel -1
